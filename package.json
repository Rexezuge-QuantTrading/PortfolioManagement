{
  "name": "@rexezuge-quant-trading/portfolio-management",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .build/ .serverless/ dist/",
    "build": "tsc",
    "predeploy": "npm run clean && npm run prettier && npm run lint && npm run build",
    "deploy": "serverless deploy",
    "prettier": "prettier --write .",
    "lint": "eslint --ignore-pattern dist/ --ignore-pattern .serverless/ --ext .ts,.tsx --fix .",
    "tsc": "tsc --noemit"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.998.x",
    "@aws-sdk/lib-dynamodb": "^3.998.x",
    "swagger-ui-dist": "^5.31.x",
    "uuid": "^13.0.x"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.x",
    "@types/node": "^25.3.x",
    "eslint": "^10.0.x",
    "eslint-config-prettier": "^10.1.x",
    "eslint-plugin-prettier": "^5.5.x",
    "globals": "^17.3.x",
    "prettier": "^3.8.x",
    "serverless": "^4.33.x",
    "typescript": "^5.9.x",
    "typescript-eslint": "^8.56.x"
  }
}
